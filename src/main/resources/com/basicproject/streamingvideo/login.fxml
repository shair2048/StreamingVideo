<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.basicproject.streamingvideo.auth.AuthController">
   <children>
      <Label layoutX="131.0" layoutY="84.0" text="Username" />
      <Label layoutX="133.0" layoutY="131.0" text="Password" />
      <Button fx:id="btnLogin" layoutX="163.0" layoutY="195.0" mnemonicParsing="false" onAction="#handleLogin" text="Login" />
      <PasswordField fx:id="txtPassword" layoutX="221.0" layoutY="127.0" />
      <TextField fx:id="txtUsername" layoutX="221.0" layoutY="80.0" />
      <Button layoutX="239.0" layoutY="195.0" mnemonicParsing="false" onAction="#switchToRegister" text="Create Account" />
      <Label fx:id="lbNotification" layoutX="236.0" layoutY="166.0" />
   </children>
</AnchorPane>
